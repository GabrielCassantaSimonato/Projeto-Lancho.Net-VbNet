    Private Sub txt_fecharpedido_Click(sender As Object, e As EventArgs) Handles txt_fecharpedido.Click
        Try
            If txt_data_pedido.Text = "" Or _
               txt_total.Text = "" Or _
               cmb_fp.Text = "" Then
                MsgBox("Preencha todos os campos do formulário!!!", MsgBoxStyle.Exclamation + MsgBoxStyle.OkOnly, "AVISO")
            Else
                resp = MsgBox("Valor total: R$ " & txt_total.Text & "" + vbNewLine & _
                          "Forma de pagamento: " & cmb_fp.Text & "" + vbNewLine & _
                          "Deseja fechar o pedido?", MsgBoxStyle.Question + MsgBoxStyle.YesNo, "ATENÇÃO")
                If resp = MsgBoxResult.Yes Then
                    sql = "insert into tb_pedidos values ('" & txt_data_pedido.Text & "', " & _
                                                             "'" & txt_total.Text & "', " & _
                                                             "'" & cmb_fp.Text & "')"


                    rs = db.Execute(UCase(sql))
                    MsgBox("Pedido gravado com sucesso!!!", MsgBoxStyle.Information + MsgBoxStyle.OkOnly, "AVISO")
                End If
            End If
        Catch ex As Exception
            MsgBox("Erro ao fazer pedido!!!", MsgBoxStyle.Critical + MsgBoxStyle.OkOnly, "AVISO")
        End Try
    End Sub
